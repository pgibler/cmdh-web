{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "vercel dev",
      "runtimeExecutable": "vercel",
      "runtimeArgs": [
        "dev"
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}